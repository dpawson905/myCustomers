<% include partials/header %>
<!-- Reg form -->
  <div class="container">
    <div class="wrapper">
      <div class="row">
        <div class="col">
          <div class="jumbotron">
            <h1 class="text-center">Password Reset</h1>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col">
          <form action="/auth/send-pw-token" method="POST">
            <div class="form-row">
              <div class="col-md-6 offset-md-3 mt-1">
                <label class="float-left" for="email">Email</label>
                <input type="email" id="email" name="email" class="form-control" placeholder="Email">
              </div>
            </div>
            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
            <button type="submit" class="btn btn-primary mt-3 full-btn float-right">Submit</button>
          </form>
        </div>
      </div>
    </div>
  </div>
<!-- End Reg form -->
<% include partials/footer %>