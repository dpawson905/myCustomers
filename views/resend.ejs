<% include partials/header %>
<div class="container">
  <div class="row">
    <div class="col m8 offset-m2">
      <h3 class="center white-text">Customer Access &mdash; Resend</h3>
    </div>
    <div class="row">
      <form class="col m8 offset-m2" action="/resend?_method=PUT" method="POST">
        <div class="row">
          <div class="input-field col m6">
            <input id="email" name="email" type="email" class="validate" required>
            <label for="email">Username</label>
            <span class="helper-text" data-error="Email address is required"></span>
          </div>
          <div class="row">
            <div class="col m12">
              <button class="waves-effect waves-light btn center-btn" type="submit">Resend Token</button>
            </div>
          </div>
        </div>

      </form>
    </div>
  </div>
</div>
<% include partials/footer %>